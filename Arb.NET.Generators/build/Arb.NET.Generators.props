<Project>

  <!--
    Initialise ArbDefault* properties to empty strings so that consumers that don't
    set them get predictable (empty) values rather than undefined property references.
    These are set here in .props so they exist before the project file is evaluated.
  -->
  <PropertyGroup>
    <ArbDefaultClass Condition="'$(ArbDefaultClass)' == ''"></ArbDefaultClass>
    <ArbDefaultNamespace Condition="'$(ArbDefaultNamespace)' == ''"></ArbDefaultNamespace>
    <ArbDefaultLangCode Condition="'$(ArbDefaultLangCode)' == ''"></ArbDefaultLangCode>
  </PropertyGroup>

</Project>
